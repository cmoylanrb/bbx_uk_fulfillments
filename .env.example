# BigQuery Configuration
BQ_PROJECT=birchbox
BQ_DATASET=automation
BQ_CREDPATH=/path/to/your/service-account-key.json
BQ_SCOPES=https://www.googleapis.com/auth/cloud-platform,https://www.googleapis.com/auth/drive
BQ_BATCH_SIZE=500

# BigQuery Tables/Views
BQ_FULFILLMENT_LINES_VIEW=automation.bbx_wh_fulfillment_lines_uk
BQ_FULFILLMENT_LIST_VIEW=automation.bbx_wh_fulfillment_list_uk
BQ_STATUS_TABLE=automation.bbx_wh_fulfillments_uk
BQ_WAVE_TABLE=automation.bbx_wh_waves_uk
BQ_SHIPMENTS_TABLE=automation.bbx_wh_shipments_uk
BQ_SHIPMENTS_VIEW=automation.bbx_wh_shipview_uk
BQ_STOCK_TABLE=automation.bbx_wh_stock_uk
BQ_STOCK_VIEW=automation.bbx_wh_stockview_uk

# Shopify Configuration
SHOPIFY_API_VERSION=2024-10
SHOPIFY_ACCESS_TOKEN=your_shopify_access_token_here
SHOPIFY_STORE_URL=https://uk-birchbox-staging.myshopify.com
SHOPIFY_LOCATION_ID=66575466553
SHOPIFY_FF_MESSAGE=Accepted.
SHOPIFY_BATCH_SIZE=50
SHOPIFY_FETCH_BATCH_SIZE=250
SHOPIFY_UPDATE_BATCH_SIZE=40

# SFTP Configuration
SFTP_HOST=sftp.prolog.co.uk
SFTP_PORT=22
SFTP_USER=birchboxsftp
SFTP_PASSWORD=your_sftp_password
SFTP_DOWNLOAD_PATH=/Live/imports-in
SFTP_DOWNLOAD_ARCHIVE_PATH=/Live/imports-in/Archive
SFTP_UPLOAD_PATH=/Live/exports-out
SFTP_UPLOAD_ARCHIVE_PATH=/Live/exports-out/Archive
STOCK_FNAME_PREFIX=BBX_Stock_Status_Report
SHIPMENTS_FNAME_PREFIX=CSH
PROLOG_FNAME_TEMPLATE={is_box_orders}BBXORD{filename_string}-PRO.txt
TSV_OUTPUT_PATH=/tmp/bbx_uk_imp

# Logging
LOG_LEVEL=INFO
MAX_RETRIES=30

# File Templates
PROLOG_FNAME_TEMPLATE={is_box_orders}BBXORD{filename_string}-PRO.txt 